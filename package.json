{
  "name": "gennyflow",
  "version": "6.1.3",
  "description": "Easily download one or more DOM elements as images in Webflow",
  "main": "src/index.ts",
  "type": "module",
  "directories": {
    "dist": "dist",
    "src": "src",
    "example": "example"
  },
  "exports": {
    ".": {
      "import": "./dist/gennyFlow.es.js",
      "require": "./dist/gennyFlow.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "watch": "vite build --watch",
    "build": "vite build && node js-to-txt-util.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/briantuckerdesign/GennyFlow.git"
  },
  "keywords": [
    "webflow",
    "image download",
    "html-to-image",
    "low code",
    "no code",
    "div to image"
  ],
  "author": "Brian Tucker",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/briantuckerdesign/GennyFlow/issues"
  },
  "homepage": "https://github.com/briantuckerdesign/GennyFlow#readme",
  "dependencies": {
    "downloadjs": "^1.4.7",
    "html-to-image": "^1.11.11",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.23.3",
    "babel-loader": "^9.1.2",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "vite": "^5.2.4"
  }
}
